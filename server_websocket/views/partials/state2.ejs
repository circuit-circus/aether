<div id="input-container" class="content-container">
    <div id="question-container">
        <div id="question-starter" class="question-element">
            <div id="question-starter-rotator">
                <span  class="focus">When will</span>
                <span>Is there</span>
                <span>Will I</span>
                <span>What is</span>
                <span>Should I</span>
                <span>Can you</span>
                <span>Could we</span>
                <span>Is it</span>
            </div>
        </div>
        <div id="question-input" class="question-element">
            <input id="question-input-field" type="text">
        </div>

        <div id="question-end" class="question-element">
            <span>?</span>
        </div>
    </div>

    <div class="help-text-container">
        <span class="help-text-main">Write a question for the universe</span>
        <br><br>
        <span class="small">
            Use the KNOB on the control panel to choose a question starter <br>
            Press the CONTINUE button when you are done
        </span>
    </div>
</div>

<%- include('popup_missing-input') %>